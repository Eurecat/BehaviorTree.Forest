<?xml version="1.0"?>
<root BTCPP_format="4" main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Repeat num_cycles="-1">
            <Sequence>
                <Sleep msec="1000"/>
                <Script code="in_switch:=1"/>
                <Sleep msec="1000"/>
                <SetBlackboard output_key="in_switch" value="0"/>
                <Sleep msec="1000"/>
                <SetBlackboard output_key="in_switch" value="1"/>
                <!-- <WasEntryUpdated entry="$${in_switch}"/> -->
                <Parallel success_count="$${in_switch}" failure_count="1">
                    <AlwaysSuccess/>
                    <AlwaysSuccess/>
                </Parallel>
            </Sequence>
        </Repeat>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <SubTree ID="BehaviorTree" description=""/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
